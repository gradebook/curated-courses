{
  "name": "@gradebook/curated-courses",
  "version": "0.0.1",
  "description": "A curated list of courses you can import into your Gradebook account",
  "repository": "https://github.com/gradebook/curated-courses",
  "author": "Vikas Potluri <vikaspotluri123.github@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "build.sh",
    "dev": "eleventy --serve --input src --output dist",
    "js:dev": "rollup -c -w",
    "lint": "xo"
  },
  "dependencies": {
    "@gradebook/course-serializer": "^0.1.0",
    "jsqr": "^1.3.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "rollup": "^2.25.0",
    "rollup-plugin-terser": "^7.0.0",
    "xo": "0.32.1"
  },
  "xo": {
    "overrides": [
      {
        "files": "src/scripts/**/*",
        "env": [
          "browser"
        ]
      }
    ]
  }
}
